// #################### String Indexing ################

let str = "Deepali";

// D e e p a l i
// 0 1 2 3 4 5 6

console.log(str[0]);
console.log(str[1]);
console.log(str[2]);
console.log(str[3]);
console.log(str[4]);
console.log(str[5]);
console.log(str[6]);

// length of String

console.log(str.length);

// last Index :
console.log(str[str.length-1]);